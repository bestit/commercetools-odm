{
    "autoload": {
        "psr-4": {
            "BestIt\\CommercetoolsODM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BestIt\\CommercetoolsODM\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "name": "bestit/commercetools-odm",
    "description": "Makes the commercetools database accessible with the doctrine commons api.",
    "type": "lib",
    "require": {
        "php": "^7.0",
        "ext-intl": "*",
        "commercetools/commons": "^0.2",
        "commercetools/php-sdk": "^2.6",
        "doctrine/common": "^2.6",
        "funct/funct": "^1.0",
        "bestit/commercetools-async-pool": "^2 || ^3",
        "psr/log": "^1.0@dev",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5 || ^6.0",
        "best-it/php_codesniffer": "^1.2 || ^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bjoern Lange",
            "email": "lange@bestit-online.de"
        }
    ],
    "minimum-stability": "beta"
}
